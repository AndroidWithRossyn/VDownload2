<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="5dp"/>
            <gradient
                android:angle="270"
                android:centerColor="#006559"
                android:endColor="#3A8077"
                android:startColor="#04423A"
                android:type="linear" />
        </shape>
    </item>
    <item
        android:left="3dp"
        android:top="3dp"
        android:right="3dp"
        android:bottom="3dp">
        <shape android:shape="rectangle">
            <corners android:radius="5dp"/>
            <gradient
                android:angle="270"
                android:centerColor="#2F8379"
                android:endColor="#399589"
                android:startColor="#053730"
                android:type="linear" />
        </shape>
    </item>
</layer-list>